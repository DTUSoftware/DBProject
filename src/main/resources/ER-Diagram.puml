@startuml
'https://plantuml.com/ie-diagram

' hide the spot
hide circle

' avoid problems with angled crows feet
skinparam linetype ortho

entity User {
  *user_email : text
  --
  *name : text
  *address : text
  *birthdate : date
  gender : boolean
}

entity Event {
  *event_id : number <<generated>>
  --
  *date : datetime
  *union_id : text <<FK>>
  *event_type_id : number <<FK>>
}

entity EventType {
  *event_type_id : number <<generated>>
  --
  *description : text
}
Event }o..|| EventType

entity Contender {
  *contender_id : number <<generated>>
  --
    * event_id : number <<FK>>
  * user_email : text <<FK>>
  time : number
}
Contender }o...|| Event
Contender }o..|| User


entity AgeGroup {
    *age_group_id : number <<generated>>
    --
    *lower_age : number
    *upper_age : number
}

entity EventType_AgeGroup {
    * age_group_id : number <<FK>>
    * event_type_id : number <<FK>>
    --
    gender : boolean
}
EventType ||..|{ EventType_AgeGroup
EventType_AgeGroup }o..|| AgeGroup

entity SportsUnion {
    *union_id : text
    --
    *name : text
    address : text
    email : text
    phone_number : text
}
SportsUnion ||..o{ Event



'User ||..o{ Event
'User |o..o{ Contender


@enduml